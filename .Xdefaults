URxvt*background: Black
URxvt*foreground: White
URxvt*scrollstyle: plain
URxvt*tintColor: white
URxvt*fading: 15
URxvt*fadeColor: black
URxvt*shading: 15
URxvt*inheritPixmap: true
URxvt*pointerColor: black
URxvt*pointerColor2: white
URxvt*borderless: true    
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true
URxvt*scrollBar: true
URxvt*scrollBar_right: true
URxvt*perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt*url-launcher: /usr/bin/google-chrome
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
!URxvt*font: 9x12
!URxvt*font: xft:droid sans mono:pixelsize=12:antialias=true:hinting=true

URxvt*termName: xterm
!URxvt*font: xft:Monospace:pixelsize=12
URxvt*font: xft:DejaVu Sans Mono:size=10
!URxvt*font: xft:mononoki:regular:size=10


!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
!Xft.hintstyle:  hintfull
Xft.hintstyle:  hintislight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault

